<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.openexpense.dao.CompanyDao">
    <select id="queryByDomain" resultType="com.openexpense.model.Company">
        SELECT * FROM oe_company WHERE company_domain = #{domain}
    </select>
</mapper>